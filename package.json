{
  "name": "goodsapp-h5-ssr",
  "version": "1.0.0",
  "description": "goodsapp ssr",
  "main": "index.js",
  "scripts": {
    "dev": "node server.js",
    "dev:next": "next",
    "build": "rimraf .next && next build",
    "start": "cross-env NODE_ENV=production node server.js",
    "export": "rimraf outCDN && next export -o outCDN",
    "copy:files": "rimraf ./deployBuildFiles/**/*.* && node ./scripts/copyFile.js",
    "upload:cdn": "node ./scripts/upload.js",
    "analyze": "cross-env ANALYZE=1 next build",
    "css:build": "webpack --config webpack-handle-css.js --progress --color",
    "css:clean": "rimraf static/styles/antd_mobile_min.js static/styles/app_min.js",
    "css:dev": "webpack --config webpack-handle-css.js --watch --progress --color",
    "pm2:start": "pm2 start npm --name \"goodsapp\" -- start",
    "gen:deploy:file": "npm run build && npm run export && npm run upload:cdn && npm run copy:files",
    "online-prev:deploy:test": "npm run gen:deploy:file && cd deployBuildFiles && npm run pm2:start",
    "deploy:dev": "pm2 deploy deploy-dev.json production setup && pm2 deploy deploy-dev.json production --force",
    "deploy:prod": "pm2 deploy deploy-prod.json production setup && pm2 deploy deploy-prod.json production --force",
    "deploy": "npm run gen:deploy:file  &&  ./deploy-app.sh && npm run deploy:prod"
  },
  "keywords": [
    "next",
    "goodsapp-ssr"
  ],
  "author": "poetry",
  "license": "ISC",
