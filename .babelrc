{
  "presets": ["next/babel"],
  "plugins": [
    [
      "import",
      { "libraryName": "antd-mobile" }
    ],
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "@components": "./components/index.js",
          "@utils": "./utils/index.js",
          "@actions": "./store/actions/index.js",
          "@storeConfig":"./store/storeConfig.js",
          "@config": "./config/index.js"
        }
      }
    ]
  ]
}
